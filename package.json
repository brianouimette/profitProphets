    "dotenv": "^16.3.1",
    "hono": "^4.0.0",
    "inngest": "^2.3.0",
    "mysql2": "^3.15.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
